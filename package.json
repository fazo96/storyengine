{
  "name": "storyengine",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:api": "deno run dev",
    "dev:vite": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "server:start": "deno run -A --watch ./src/api/main.ts",
    "serve": "deno run build && deno run server:start",
    "preview": "vite preview"
  },
  "dependencies": {
    "@deno/vite-plugin": "^1.0.5",
    "dompurify": "^3.3.0",
    "marked": "^16.4.1",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  }
}
